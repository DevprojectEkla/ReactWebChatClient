{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "ignore",
  "quoteProps": "consistent",
  "embeddedLanguageFormatting": "auto",
  "jsxSingleQuote": true,
  "proseWrap": "always",
  "eslintIntegration": true,
  "vueIndentScriptAndStyle": true
}
